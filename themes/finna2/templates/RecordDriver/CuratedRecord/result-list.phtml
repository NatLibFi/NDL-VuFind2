<?php $records = $this->driver->getEncapsulatedRecords(); ?>
<?php if ($record = reset($records)): ?>
  <?php if ($record instanceof \VuFind\RecordDriver\Missing): ?>
    <div class="media">
      <div class="media-body">
        <?= $this->transEsc('Cannot find record') . ': ' . $this->escapeHtmlAttr($record->getUniqueID()) ?>
      </div>
    </div>
  <?php else: ?>
    <?= $this->record($record)->getSearchResult('list') ?>
  <?php endif; ?>
<?php endif; ?>
